<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"> 
<mapper namespace="com.yklis.dao.AppVisitDao">  

    <insert id="insertAppVisit" parameterType="com.yklis.entity.AppVisitEntity">
        insert into AppVisit 
        ( 
    sysName
    ,pageName
    ,ip
    ,customer
    ,userName
    ,actionName
    ,actionTime
    ,remark
    ,reserve
    ,reserve2
    ,reserve3
    ,reserve4
    ,reserve5
    ,reserve6
    ,reserve7
    ,reserve8
    ,reserve9
    ,reserve10
        )
         VALUES
        (
    #{sysName},
    #{pageName},
    #{ip},
    #{customer},
    #{userName},
    #{actionName},
    #{actionTime},
    #{remark},
    #{reserve},
    #{reserve2},
    #{reserve3},
    #{reserve4},
    #{reserve5},
    #{reserve6},
    #{reserve7},
    #{reserve8},
    #{reserve9},
    #{reserve10}        
        )   
    </insert>
        
</mapper>